    "# Extracting data we want from the table for all programs and clean it up\n",
    "\n",
    "data_sanitized = data.copy()\n",
    "\n",
    "for folder_name, folder in data_sanitized.items():\n",
    "\n",
    "    if folder_name == \"Majors\": continue # we only want to process the survey data, not the student-major database\n",
    "\n",
    "    for program_name, program_df in folder.items():\n",
    "\n",
    "        program_df = program_df.drop([0,1]) # drop non relevant rows that don't contain responses\n",
    "\n",
    "        columns_to_keep = [\"RecipientEmail\", \"Q2\", \"Q3\", \"Q15\", \"Q5\"]\n",
    "        program_df = program_df[columns_to_keep]\n",
    "\n",
    "        # Apply regex to keep only the part before \"@\"\n",
    "        program_df['RecipientEmail'] = program_df['RecipientEmail'].str.replace(r'@.*', '', regex=True)\n",
    "        program_df['RecipientEmail'] = program_df['RecipientEmail'].str.lower()\n",
    "        \n",
    "        program_df = program_df.rename(columns={'RecipientEmail': 'Uniqname'}) # dataframe with only the above columns for a given program\n",
    "\n",
    "        data_sanitized[folder_name][program_name] = program_df # assign back to dictionary\n",
    "\n",
    "# print(data_sanitized)\n"
   ]
