    "# Sanitizing names and getting data into dataframes\n",
    "\n",
    "current_folder = os.getcwd()\n",
    "\n",
    "folder_path_SU23 = os.path.join(current_folder, "SU23Arrival")\n"
    "folder_path_SU24 = os.path.join(current_folder, "SU24Arrival")\n"
    "folder_path_WN23 = os.path.join(current_folder, "WN23Arrival")\n"
    "folder_path_WN24 = os.path.join(current_folder, "WN24Arrival")\n"
    "folder_path_PreArrival = os.path.join(current_folder, "PreArrival")\n"
    "folder_path_Majors = os.path.join(current_folder, "Majors")\n"
    "\n",
    "SU23 = glob.glob(os.path.join(folder_path_SU23, \"*.xlsx\"))\n",
    "SU24 = glob.glob(os.path.join(folder_path_SU24, \"*.xlsx\"))\n",
    "WN23 = glob.glob(os.path.join(folder_path_WN23, \"*.xlsx\"))\n",
    "WN24 = glob.glob(os.path.join(folder_path_WN24, \"*.xlsx\"))\n",
    "#PreArrival = glob.glob(os.path.join(folder_path_PreArrival, \"*.xlsx\"))\n",
    "Majors     = glob.glob(os.path.join(folder_path_Majors, \"*.xlsx\"))\n",
    "\n",
    "folders = {\n",
    "    \"SU23\": SU23,\n",
    "    \"SU24\": SU24,\n",
    "    \"WN23\": WN23,\n",
    "    \"WN24\": WN24,\n",
    "    \"Majors\":Majors\n",
    "}\n",
    "\n",
    "data = {}\n",
    "\n",
    "for folder_name, folder in folders.items():  # Assign each the correct name\n",
    "    \n",
    "    #print(folder_name)\n",
    "    data[folder_name] = {}\n",
    "\n",
    "    for datasheet in folder:\n",
    "        # Extract filename without extension\n",
    "        file_name = os.path.splitext(os.path.basename(datasheet))[0]\n",
    "        \n",
    "        # Replace '+' with '_'\n",
    "        sanitized_name = file_name.replace(\"+\", \"_\")\n",
    "        sanitized_name = sanitized_name.replace(\"-\", \"_\")\n",
    "        sanitized_name = re.sub(r'_+', '_', sanitized_name).strip('_')        \n",
    "\n",
    "        # Normalize capitalization (convert to lowercase then title case)\n",
    "        sanitized_name = sanitized_name.lower().title()\n",
    "\n",
    "        # remove arrival or survey\n",
    "        sanitized_name = re.sub(r'(?i)[ _\\+\\-]*(arrival|survey)[ _\\+\\-]*', ' ', sanitized_name).strip()\n",
    "        sanitized_name = re.sub(r'\\s+', '_', sanitized_name).strip()\n",
    "\n",
    "        # Remove anything after \"February\"\n",
    "        sanitized_name = re.sub(r'February.*', '', sanitized_name).strip()\n",
    "\n",
    "        # Remove trailing underscores\n",
    "        sanitized_name = sanitized_name.rstrip(\"_\")\n",
    "\n",
    "        #replace w23 at end\n",
    "        match = re.search(r'(_WN23|_SU23|_WN24|_SU24)$', sanitized_name, re.IGNORECASE)\n",
    "        if match:\n",
    "            suffix = match.group(0)  # Extracts \"_WN23\"\n",
    "            sanitized_name = suffix[1:] + \"_\" + sanitized_name[: -len(suffix)]  # Moves it to the front\n",
    "\n",
    "        # edge cases\n",
    "        if re.match(r\"Aerospace_Summer_Program$\", sanitized_name, re.IGNORECASE):\n",
    "            sanitized_name = 'Su23_Aerospace_France'\n",
    "\n",
    "        if re.match(r\"Ipe_Winter_23_Pre_Departure_Orientation$\", sanitized_name, re.IGNORECASE):\n",
    "            sanitized_name = 'Wn23_Pdo'\n",
    "\n",
    "        sanitized_name = re.sub(r'_?\\(updated\\)_?', '', sanitized_name, flags=re.IGNORECASE)\n",
    "\n",
    "        #print(sanitized_name) # for debuggging\n",
    "\n",
    "        # Read Excel file into a DataFrame\n",
    "        df = pd.read_excel(datasheet)\n",
    "        \n",
    "        # Store in dictionary\n",
    "        data[folder_name][sanitized_name] = df"
   ]
