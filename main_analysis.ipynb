    "def sanitize_filename(name):\n",
    "    # Replace any character that is not a letter, number, or space with an underscore\n",
    "    sanitized_name = re.sub(r'[\\\\/*?:\"<>|]', '_', name)  # Replace forbidden characters\n",
    "    sanitized_name = sanitized_name.strip()  # Remove leading/trailing spaces\n",
    "    return sanitized_name\n",
    "\n",
    "# function to generate plots\n",
    "def make_bar_plot(counts, question_name, major_name, answer_order=None, save_folder='plots'):\n",
    "    sanitized_question_name = sanitize_filename(question_name)\n",
    "    sanitized_major_name = sanitize_filename(major_name)\n",
    "\n",
    "    # Create the subdirectory save_folder inside 'plots'\n",
    "    save_folder_path = os.path.join('Plots', save_folder)\n",
    "\n",
    "    # Check if the save_folder inside 'plots' exists, create it if not\n",
    "    if not os.path.exists(save_folder_path):\n",
    "        os.makedirs(save_folder_path)\n",
    "\n",
    "    plt.figure(figsize=(10, 6))\n",
    "\n",
    "    if answer_order:\n",
    "        counts = counts.reindex(answer_order, fill_value=0)\n",
    "\n",
    "    colors = plt.colormaps['tab20']\n",
    "    bars = counts.plot(kind='bar', color=colors(range(len(counts))), edgecolor='black')\n",
    "\n",
    "    plt.ylim(0, 110)  # Keep y-axis fixed from 0 to 100\n",
    "    plt.yticks(range(0, 101, 20))  # Set increments of 20\n",
    "\n",
    "    plt.xlabel('Response')\n",
    "    plt.ylabel('Percentage of students (%)')\n",
    "\n",
    "    # Set title with top padding retained\n",
    "    title_text = f\"{sanitized_question_name}\\n{sanitized_major_name}\"\n",
    "    plt.title(title_text, fontsize=14, pad=30)  # Retained padding\n",
    "\n",
    "    plt.xticks(rotation=45, ha='right')\n",
    "\n",
    "    for bar in bars.patches:\n",
    "        height = bar.get_height()\n",
    "        plt.text(\n",
    "            bar.get_x() + bar.get_width() / 2,\n",
    "            height+1,  # Prevents labels from going off the top\n",
    "            f'{height:.1f}%',\n",
    "            ha='center', va='bottom', fontsize=10\n",
    "        )\n",
    "\n",
    "    file_path = os.path.join(save_folder_path, f\"{sanitized_question_name}_{sanitized_major_name}.png\")\n",
    "    plt.tight_layout(pad=3)\n",
    "    plt.savefig(file_path)\n",
    "    plt.close()\n"
   ]
